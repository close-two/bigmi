$(".n-main-nav li").each(function(c,e){var d=$(e);var g=location.pathname;var f=g.substring(11,g.length-5);if(!d.hasClass("current")){var b=d.find("a");b.attr("href","/pass/auth/"+b.data("action")+"/home"+location.search)}if(f===b.data("action")){d.addClass("current")}});$(".nav-item").each(function(){$(this).attr("href","/pass/auth/"+$(this).data("action")+"/home"+location.search)});